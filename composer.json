{
    "name": "phoenixseo/de-pack",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "drupal/simple_timeline": "^1",
        "drupal/amp": "^3",
        "drupal/address": "^1",
        "drupal/amptheme": "^3",
        "drupal/admin_toolbar": "^1",
        "drupal/better_exposed_filters": "^3",
        "drupal/block_class": "^1",
        "drupal/dropzonejs": "^2",
        "drupal/embed": "^1",
        "drupal/entity_browser": "^2",
        "drupal/entity_embed": "^1",
        "drupal/entity_reference_revisions": "^1",
        "drupal/entity_reference_views_select": "^1",
        "drupal/entityqueue": "^1",
        "drupal/eu_cookie_compliance": "^1",
        "drupal/field_group": "^3",
        "drupal/file_entity": "^2",
        "drupal/file_browser": "^1",
        "drupal/inline_entity_form": "^1",
        "drupal/linkit": "^5",
        "drupal/metatag": "^1",
        "drupal/paragraphs": "^1",
        "drupal/pathauto": "^1",
        "drupal/yoast_seo": "^2",
        "drupal/redirect": "^1",
        "drupal/schema_metatag": "^1",
        "drupal/simple_sitemap": "^3",
        "drupal/superfish": "^1",
        "drupal/svg_image": "^1",
        "drupal/tvi": "^1",
        "drupal/token": "^1",
        "drupal/video_embed_field": "^2",
        "drupal/views_flipped_table": "^1",
        "drupal/webform": "^5",
        "drupal/entity_reference_tree": "^1",
        "drupal/views_tree": "^2",
        "drupal/imageapi_optimize_resmushit": "^1",
        "drupal/lazy": "^2",
        "dinbror/blazy": "1.8.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "comments": {
            "drupal/lazy": "Bilder \"nachladen\" mit JavaScript Bibliothek \"blazy\""
        }
    },
    "require-dev": {
        "drupal/optimizedb": "^1"
    },
    "authors": [
        {
            "name": "Frank Pfabigan",
            "email": "frank.pfabigan@gmail.com",
            "homepage": "https://phoenixseo.de",
            "role": "Berater Online-Marketing"
        }
    ],
    "homepage": "https://www.de-pack.de"
}
